<body class="theme-dark io-azure io-light-pro" data-spy="scroll" data-target="#mainnav" data-offset="80">

<div class="user-page d-flex align-items-center">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-9">
                <div class="user-pro-box user-pro-box-s2">
                    <div class="row">
                        <div class="col-lg-4">
                            <div class="user-pro-info">
                                <div class="user-logo">
                                    <a href="#">
                                        <img src="images/extra-pages/logo-sm-white.png"  srcset="images/extra-pages/logo-sm-white2x.png 2x" alt="icon">
                                    </a>
                                </div>
                                <div class="user-graphics">
                                    <img src="images/extra-pages/user-page-image-b.png" alt="user">
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-8">
                            <div class="user-pro-form">
                                <div class="tab-content" id="nav-tabContent">
                                    <h4 style="color: black" align="center">Login to Your Account</h4>
                                    <div class="tab-pane fade show active" id="cont-signin" role="tabpanel">
                                        <span class="form-heading">Please use your correct inaaaformation to login.</span>
                                        <form id="loginForm" class="login-form">
                                            <div class="input-item">
                                                <input type="text" name="emailOrUsername" placeholder="Email Or Username" class="input-border-simple">
                                                <em class="fas fa-user"></em>
                                            </div>
                                            <div class="input-item">
                                                <input type="password" name="password" placeholder="Password" class="input-border-simple">
                                                <em class="fas fa-lock"></em>
                                            </div>
                                            <div class="d-flex justify-content-between align-items-center">
                                                <button class="btn btn-sm" type="submit">Log in</button>
                                                <a class="form-link" href="forgot-password">Forgot Password?</a>
                                            </div>
                                            <div class="gaps"></div>
                                            <span class="text-center d-block">Not a member? <a href="signup" class="simple-link">Sign up now</a></span>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="gaps"></div>
                <div class="form-footer d-flex justify-content-between">
                    <span>Copyright Â© 2018 ICO Crypto.</span>
                    <ul class="form-footer-links">
                        <li><a href="">Privacy Policy</a></li>
                        <li><a href="">Terms &amp; Conditions</a></li>
                    </ul>
                </div>
            </div>
